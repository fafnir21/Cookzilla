<%- include('header') %>

<div class="ui grid">

  <div class="eight wide column">
    <form class="ui form segment" method="post">
      <div class="field">
        <label>Address</label>
        <input type="text" value="<%- profile[0].address %>" name="address">
      </div>
      <div class="field">
        <label>Birthday</label>
        <input type="date" value="<%- date %>" name="birthday">
      </div>
      <div class="field">
        <label>Phone</label>
        <input type="text" value="<%- profile[0].phone %>" name="phone">
      </div>
      <input type="submit" class="ui button" value="submit">
    </div>
  </form>
</div>

<%- include('footer') %>
